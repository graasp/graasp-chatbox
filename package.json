    "@mui/material": "^5 || ^6.0.0",
    "@tanstack/react-query": "^4",
    "date-fns": "^3.2.0 || ^4.0.0",
    "i18next": "^23.7.0 || ^24.0.0",
    "lucide-react": "*",
    "react": "*",
    "react-dom": "*",
    "react-i18next": "^13.0.0 || ^14.0.0 || ^15.0.0"
