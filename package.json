    "react-mentions": "4.4.7",
    "react-query": "3.39.3",
    "remark-breaks": "3.0.2",
    "remark-gfm": "3.0.1",
    "tsc-alias": "1.8.2"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
