  "license": "AGPL3.0-only",
  "bugs": {
    "url": "http://github.com/graasp/graasp-chatbox/issues"
  },
  "workspaces": [
    "example"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc",
    "start": "rimraf dist && tsc && rollup --watch -c rollup.config.js",
    "start:example": "yarn workspace graasp-chatbox-example start",
    "prepack": "yarn build",
    "prepare": "yarn prepack",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "check": "yarn prettier:check && yarn lint && yarn type-check",
    "prettier:check": "prettier --check \"{src,example}/**/*.{js,ts,tsx}\"",
    "prettier:write": "prettier --write \"{src,example,cypress}/**/*.{js,ts,tsx}\"",
    "pretest": "cd example && yarn",
    "test": "yarn build",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "cypress:open": "cypress open",
    "cypress:ci": "cypress run --component"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "*"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
