{
  "name": "@graasp/chatbox",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "index.ts",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "repository": "graasp/graasp-chatbox",
  "author": "Kim Lan Phan Hoang",
  "license": "MIT",
  "workspaces": [
    "example"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "pre-commit": "yarn prettier:check && yarn lint",
    "post-commit": "git status",
    "build": "rimraf dist && tsc && rollup -c rollup.config.js",
    "start": "rimraf dist && tsc && rollup --watch -c rollup.config.js",
    "start:example": "yarn workspace graasp-chatbox-example start",
    "prepack": "yarn build",
    "prepare": "yarn prepack",
    "lint": "eslint .",
    "prettier:check": "prettier --check \"{src,example}/**/*.{js,ts,tsx}\"",
    "prettier:write": "prettier --write \"{src,example}/**/*.{js,ts,tsx}\"",
    "pretest": "cd example && yarn",
    "test": "yarn build && yarn test:unit",
    "test:unit": "cross-env CI=1 jest src --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "predeploy": "cd example && yarn install && yarn build",
    "release": "standard-version",
    "cypress": "cypress open-ct",
    "cypress:ci": "cypress run-ct"
  },
  "peerDependencies": {
    "@material-ui/core": "*",
    "@material-ui/icons": "*",
    "@material-ui/lab": "*",
    "@testing-library/dom": "*",
    "eslint": "*",
    "react": "*",
    "react-dom": "*",
    "tslib": "*"
  },
  "dependencies": {
    "@graasp/translations": "github:graasp/graasp-translations.git",
    "clsx": "1.1.1",
    "i18next": "21.6.11",
    "immutable": "4.0.0",
    "lodash.truncate": "4.4.2",
    "moment": "2.29.1",
    "react-i18next": "11.15.3"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "devDependencies": {
    "@commitlint/cli": "16.0.2",
    "@commitlint/config-conventional": "16.0.0",
    "@cypress/instrument-cra": "1.4.0",
    "@cypress/react": "5.10.3",
    "@cypress/webpack-dev-server": "1.7.0",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-typescript": "8.3.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/lodash.truncate": "4.4.6",
    "@types/material-ui": "0.21.12",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.11",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "cross-env": "7.0.3",
    "cypress": "9.1.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-app": "^6.2.2",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "rimraf": "3.0.2",
    "rollup": "2.63.0",
    "rollup-jest": "1.1.3",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "standard-version": "9.3.2",
    "ts-jest": "27.1.2",
    "ts-loader": "9.2.7",
    "typescript": "4.3.5"
  },
  "files": [
    "dist"
  ],
  "packageManager": "yarn@3.2.0-rc.10"
}
