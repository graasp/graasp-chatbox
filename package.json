{
  "name": "@graasp/chatbox",
  "version": "1.1.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typescript": {
    "definition": "./dist/index.d.ts"
  },
  "repository": "graasp/graasp-chatbox",
  "author": "Graasp",
  "contributors": [
    "Kim Lan Phan Hoang",
    "Basile Spaenlehauer"
