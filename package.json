{
  "name": "@graasp/chatbox",
  "version": "2.0.0",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typescript": {
    "definition": "./dist/index.d.ts"
  },
  "repository": "graasp/graasp-chatbox",
  "author": "Graasp",
